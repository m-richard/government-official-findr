<h1>Enter Your Information Here</h1>

<%= form_for(@address) do |f| %>

  <%= f.label :street %>
  <%= f.text_field :street %>

  <%= f.label :city %>
  <%= f.text_field :city %>

  <%= f.label :state %>
  <%= f.text_field :state %>

  <%= f.label :zip_code %>
  <%= f.text_field :zip_code %>

  <%= f.submit "Submit" %>
<% end %>

<%= flash[:notice] %>
